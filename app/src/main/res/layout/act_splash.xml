<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >

  <data>
    <variable
        name="view"
        type="com.timper.lonelysword.app.feature.splash.SplashActivity"
        />
    <variable
        name="viewModel"
        type="com.timper.lonelysword.app.feature.splash.SplashViewModel"
        />
  </data>

  <android.support.constraint.ConstraintLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      tools:context="com.timper.lonelysword.lonelysword.com.timper.lonelysword.app.MainActivity"
      >

    <FrameLayout
        android:id="@+id/fl_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

      <TextView
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:text="@{viewModel.hellow}"
          />

    </FrameLayout>

  </android.support.constraint.ConstraintLayout>
</layout>